<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">

    <v:String x:Key="SerializeButtonText">Сериализовать</v:String>
    <v:String x:Key="DeserializeButtonText">Десериализовать</v:String>
    <v:String x:Key="SerializationSuccessMessage">Данные успешно сериализованы!</v:String>
    <v:String x:Key="SerializationFailureMessage">Ошибка сериализации: {0}</v:String>
    <v:String x:Key="DeserializationSuccessMessage">Данные успешно десериализованы!</v:String>
    <v:String x:Key="DeserializationFailureMessage">Ошибка десериализации: {0}</v:String>
    <v:String x:Key="SerializedFileNotFoundMessage">Файл сериализации не найден.</v:String>
</ResourceDictionary>
